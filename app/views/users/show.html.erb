      <strong>User Name:</strong> <%= @user.name %> </br >
      <strong>Email:</strong> <%= @user.email %> </br >
      <strong>Phone Number:</strong> <%= @user.phone_number %>
    </p>
  </div>
</div> 

<div class="row">

      <% @user.things.each do |thing| %>
          <div class="large-4 columns">
              <% if thing.avatar.nil? %>
                  <%= image_tag thing.thing_type.avatar.url %>
              <% else %>
                  <%= image_tag thing.avatar.url %>
              <% end %>
              <h4><%= thing.name %> (<%= thing.thing_type.name %>)</h4>
              <% if thing.description.nil? or thing.description == "" %>
                  <p><em><%= "No Description" %></em></p>
              <% else %>
                  <p><%= thing.description %></p>
              <% end %>
          </div>
          <!-- Add Report Missing Button -->
      <% end %>
